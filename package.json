    "unit-test": "mocha --compilers js:babel-register --require ./test/helpers.js --require ./test/dom.js --require ignore-styles --recursive 'src/**/*spec.js?(x)'",
    "integration-test": "mocha --compilers js:babel-register --require ./test/helpers.js --require ./test/dom.js --require ignore-styles --recursive 'test/**/*spec.js?(x)'",
    "test": "npm run unit-test",
    "precommit": "npm run lint && npm run unit-test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
